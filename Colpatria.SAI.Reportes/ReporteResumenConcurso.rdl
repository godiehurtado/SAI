    <DataSet Name="PremioxSubregla">
      <Query>
        <DataSourceName>SAI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@concurso_id">
            <Value>=Parameters!concurso_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        p.descripcion AS descripcionPremio, sr.descripcion AS descripcionSubregla
					, r.nombre AS regla 
FROM            Premio AS p INNER JOIN
                         PremioxSubregla AS ps ON ps.premio_id = p.id INNER JOIN
                         SubRegla AS sr ON sr.id = ps.subregla_id INNER JOIN
                         Regla AS r ON r.id = sr.regla_id
WHERE        (r.concurso_id = @concurso_id)</CommandText>
      </Query>
