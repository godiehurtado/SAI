        </Field>
        <Field Name="regla">
          <DataField>regla</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Condicion">
      <Query>
        <DataSourceName>SAI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@concurso_id">
            <Value>=Parameters!concurso_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        c.descripcion
	, sr.descripcion AS subregla
	, r.nombre AS regla
FROM            Condicion AS c INNER JOIN
                         SubRegla AS sr ON sr.id = c.subregla_id INNER JOIN
                         Regla AS r ON r.id = sr.regla_id
WHERE        (r.concurso_id = @concurso_id)</CommandText>
      </Query>
