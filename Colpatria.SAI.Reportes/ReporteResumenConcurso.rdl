  <DataSets>
    <DataSet Name="Concurso">
      <Query>
        <DataSourceName>SAI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@concurso_id">
            <Value>=Parameters!concurso_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        nombre, fecha_inicio, fecha_fin
FROM            Concurso AS c
WHERE        (id = @concurso_id)</CommandText>
      </Query>
      <Fields>
        <Field Name="nombre">
          <DataField>nombre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fecha_inicio">
          <DataField>fecha_inicio</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="fecha_fin">
          <DataField>fecha_fin</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReglaxConcurso">
      <Query>
        <DataSourceName>SAI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@concurso_id">
            <Value>=Parameters!concurso_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        r.nombre AS regla, r.fecha_inicio, r.fecha_fin, tr.nombre AS tipoRegla, pr.periodo AS periodoRegla
FROM            Regla AS r INNER JOIN
                         TipoRegla AS tr ON tr.id = r.tipoRegla_id INNER JOIN
                         PeriodoRegla AS pr ON pr.id = r.periodoRegla_id
WHERE        (r.concurso_id = @concurso_id)</CommandText>
      </Query>
      <Fields>
        <Field Name="regla">
          <DataField>regla</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fecha_inicio">
          <DataField>fecha_inicio</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="fecha_fin">
          <DataField>fecha_fin</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="tipoRegla">
          <DataField>tipoRegla</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="periodoRegla">
          <DataField>periodoRegla</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SubReglaxRegla">
      <Query>
        <DataSourceName>SAI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@concurso_id">
            <Value>=Parameters!concurso_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        sr.descripcion, CASE WHEN sr.principal = 1 THEN 'SI' ELSE 'NO' END AS Principal, 
                         CASE WHEN sr.tipoSubregla = 1 THEN 'Simple' WHEN sr.tipoSubregla = 2 THEN 'Agrupada' WHEN sr.tipoSubregla = 3 THEN 'Excepcion' END AS tipoSubregla, 
                         r.nombre AS regla
FROM            SubRegla AS sr INNER JOIN
                         Regla AS r ON r.id = sr.regla_id
WHERE        (r.concurso_id = @concurso_id)</CommandText>
      </Query>
      <Fields>
        <Field Name="descripcion">
          <DataField>descripcion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Principal">
          <DataField>Principal</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tipoSubregla">
          <DataField>tipoSubregla</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="regla">
          <DataField>regla</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CondicionAgrupada">
      <Query>
        <DataSourceName>SAI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@concurso_id">
            <Value>=Parameters!concurso_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ca.nombre AS condicionAgrupada, sr1.descripcion AS subregla1, o.nombre AS operador, sr2.descripcion AS subregla2, r.nombre AS regla
FROM            CondicionAgrupada AS ca INNER JOIN
                         SubRegla AS sr1 ON sr1.id = ca.subregla_id1 INNER JOIN
                         SubRegla AS sr2 ON sr2.id = ca.subregla_id2 INNER JOIN
                         Operador AS o ON o.id = ca.operador_id INNER JOIN
                         Regla AS r ON r.id = ca.regla_id
WHERE        (r.concurso_id = @concurso_id)</CommandText>
      </Query>
      <Fields>
        <Field Name="condicionAgrupada">
          <DataField>condicionAgrupada</DataField>
          <rd:TypeName>System.String</rd:TypeName>
